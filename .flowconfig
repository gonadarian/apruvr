[include]
.*/src/app/.*

[ignore]
<PROJECT_ROOT>/node_modules/.*
<PROJECT_ROOT>/flow-typed/.*
<PROJECT_ROOT>/flow-coverage/.*
<PROJECT_ROOT>/src/firebase/.*
<PROJECT_ROOT>/src/node/.*
<PROJECT_ROOT>/src/public/.*
<PROJECT_ROOT>/test/.*
